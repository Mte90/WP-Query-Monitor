{
  "name": "WordPress Query Monitor",
  "version": "0.1.0",
  "manifest_version": 2,
  "description": "The WordPress plugin Query Monitor is better on the browser DevTools, trust me!",
  "icons": {
    "48": "toast.png"
  },
  "content_scripts": [
    {
      "js": ["devtools/contentscript.js"],
      "run_at": "document_idle",
      "matches": ["<all_urls>"]
    }
],
  "permissions": [
    "<all_urls>"
  ],
  "devtools_page": "devtools/devtools-page.html"
}